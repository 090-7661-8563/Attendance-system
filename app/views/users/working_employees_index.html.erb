<% provide(:title, '出勤中社員一覧') %>
<h1>出勤中社員一覧</h1>
<div class="row">
  <%= paginate @users %>
  <!-- ヘッドのテーブル部分 -->    
  <table class="table table-bordered table-striped table-condensed">
    <thead>
      <tr>
        <th>社員番号</th>
        <th>名前</th>
      </tr>
    </thead>
    <tbody>
    <!--each文-->
    <% @users.each do |user| %>
      <tr>
        <%# if user.attendancestarted_time.today.presence && user.finished_time.today.nil %>
        <td><%= user.employee_number %></td>
        <td><%= user.name %></td>
        <%# end %>
      </tr>
    <% end %>
    </tbody>
  </table>
  <%= paginate @users %>
</div>

