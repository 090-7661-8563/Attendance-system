<% provide(:title, '社員一覧') %>
<h1>社員一覧</h1>
<div>
  <%= form_tag csv_import_path, method: :post, multipart: true do |f| %>
      <%= file_field_tag 'csv_file', style: "width: 250px;", class: %i(csv_input_btn), onchange: "file_selected($(this));" %>
      <%= submit_tag "CSVをインポート" ,style: "width: 250px; color:white;", class: "btn btn-sm btn-primary"%>
  <% end %>
</div>

<%= paginate @users %>

